<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Front 1-1 -->
  <ThingDef Name="TrenchBase" Abstract="True">
    <category>Building</category>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <selectable>true</selectable>
    <drawerType>MapMeshAndRealTime</drawerType>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <repairEffect>Repair</repairEffect>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
  </ThingDef>
  <ThingDef ParentName="TrenchBase">
    <defName>TrenchesAtlas11tr</defName>
    <label>Trench - North/East</label>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <description>Trenches for extra cover.</description>
    <designationCategory>Trenches</designationCategory>
    <researchPrerequisites>
      <li>Trenches1</li>
    </researchPrerequisites>
    <graphicData>
      <texPath>Things/Building/Trench/Trenches/Trench11_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <linkType>Basic</linkType>
      <linkFlags>
        <li>Custom2</li>
      </linkFlags>
    </graphicData>
    <uiIconPath>Things/Building/Trench/Trenches/trench11_UI</uiIconPath>
    <constructEffect>ConstructWood</constructEffect>
    <repairEffect>ConstructWood</repairEffect>
    <altitudeLayer>Building</altitudeLayer>
    <building>
      <isInert>true</isInert>
      <blueprintGraphicData>
        <texPath>Things/Building/Trench/Trenches/Blueprint/Trench11_Blueprint_Atlas</texPath>
      </blueprintGraphicData>
    </building>
    <statBases>
      <Beauty>-10</Beauty>
      <MaxHitPoints>900</MaxHitPoints>
      <WorkToBuild>500</WorkToBuild>
      <Flammability>0.5</Flammability>
    </statBases>
    <costList>
      <WoodLog>10</WoodLog>
      <Steel>4</Steel>
    </costList>
    <damageMultipliers>
      <li>
        <damageDef>Bomb</damageDef>
        <multiplier>10</multiplier>
      </li>
    </damageMultipliers>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <passability>PassThroughOnly</passability>
    <pathCost>80</pathCost>
    <pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
    <castEdgeShadows>false</castEdgeShadows>
    <fillPercent>0.8</fillPercent>
    <coversFloor>false</coversFloor>
    <placingDraggableDimensions>1</placingDraggableDimensions>
    <tickerType>Never</tickerType>
    <rotatable>false</rotatable>
    <selectable>true</selectable>
    <neverMultiSelect>false</neverMultiSelect>
    <holdsRoof>false</holdsRoof>
    <staticSunShadowHeight>0</staticSunShadowHeight>
    <blockLight>false</blockLight>
  </ThingDef>
